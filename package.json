{
  "name": "kinocms",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "deploy": "vite build && gh-pages -d dist",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "styles": "stylelint \"**/*.scss\" --fix",
    "generate-api-windows": "npx @openapitools/openapi-generator-cli generate -i ./src/shared/api/swagger.json -o ./src/shared/api/gen -g typescript-axios --additional-properties=supportsES6=true,useSingleRequestParameter=true"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "js-cookie": "^3.0.5",
    "pinia": "^2.1.7",
    "vee-validate": "^4.13.0",
    "vue": "^3.4.21",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.0",
    "vue-toastification": "^2.0.0-rc.5",
    "vue3-toastify": "^0.2.1",
    "vuetify": "^3.6.6",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@mdi/font": "^7.4.47",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "css-loader": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "font-awesome": "^4.7.0",
    "gh-pages": "^6.1.1",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "sass": "^1.77.1",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "stylelint": "^16.5.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.3.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vue-tsc": "^2.0.11"
